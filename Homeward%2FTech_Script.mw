# Creates a gallery button.
screen cg_button:
    
    $ action = g.Action(name)
    
    button:
        style "default"
        action action
        xmaximum (287 + 20)
        ymaximum (155 + 10)
        xminimum (287 + 20)
        yminimum (155 + 10)
        xmargin 10
        ymargin 5
        idle_background "UI Graphics/cggallery_buttonidle.png"
        hover_background "UI Graphics/cggallery_buttonhover.png"
        
        if action:
            add thumb xalign 0.5 yalign 0.5
        
        
    
screen gallery_common:
    imagemap:
        ground "UI Graphics/cggallery_ground.jpg"
        idle "UI Graphics/cggallery_idle.jpg"
        hover "UI Graphics/cggallery_hover.jpg"
        selected_idle "UI Graphics/cggallery_selectidle.jpg"
        selected_hover "UI Graphics/cggallery_selecthover.jpg"

        # EDIT POINT 2 - Navigation.
        
        # Return
        hotspot (932, 681, 290, 63) action ShowMenu("bonus_menu")
        
        # Waifu picker.
        hotspot (794, 218, 234, 59) action ShowMenu("gallery_general1")
        hotspot (794, 284, 234, 59) action ShowMenu("gallery_nonami1")
        hotspot (794, 353, 234, 59) action ShowMenu("gallery_haruka1")
        hotspot (794, 421, 234, 59) action ShowMenu("gallery_sora1")
        hotspot (794, 487, 234, 59) action ShowMenu("gallery_cut")
        hotspot (794, 553, 234, 59) action Start("opvideo")
        
        # Previous, Next
        if previous:
            hotspot (336, 168, 201, 36) action ShowMenu(previous)
        if next:
            hotspot (337, 708, 201, 36) action ShowMenu(next)

