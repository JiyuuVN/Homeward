##############################################################################
# Tout - by doing Show('tout', message='My Message'), we cause a message to
# show up in the upper-left corner of the screen for 2 seconds, then to
# automatically hide.

# The transform used for the tout text.
transform tout_transform:
    on show:
        alpha 0.0
        linear 0.25 alpha 1.0
    on hide:
        linear 0.25 alpha 0.0

# The tout screen.
screen tout:
    zorder 1

    text message:
        at tout_transform
        align (.01, .01) 
        size 30
        outlines [ (2, "#000") ]
        
    timer 4.0 action Hide('tout')

##############################################################################
# The customized say screen.
    
screen say:
    imagemap:
        # The height of the imagemap.
        ymaximum 233

        # Align things vertically.
        yalign 1.0
        
        ground "UI Graphics/ui_messagebox.png"
        idle "UI Graphics/ui_buttons_default.png"
        insensitive "UI Graphics/ui_buttons_default.png"
        hover "UI Graphics/ui_buttons_highlighted.png"
        selected_idle "UI Graphics/ui_buttons_highlighted.png"
        selected_hover "UI Graphics/ui_buttons_highlighted.png"
                 
        hotspot (15, 77, 88, 25) action [ FileSave("1", page="quick", newest=False, confirm=False),
                                          Show('tout', message="Quick Save completed.") ]
        hotspot (31, 113, 88, 25) action FileLoad("1", page="quick", confirm=True)
        hotspot (56, 148, 88, 25) action ShowMenu('save')
        hotspot (82, 183, 88, 25) action ShowMenu('load')

        # Log (850, 207, 65, 13)

        hotspot (920, 203, 86, 25) action Skip()
        hotspot (1007, 203, 86, 25) action Preference("auto-forward", "toggle")
        hotspot (1092, 203, 86, 25) action [ Screenshot(), Show('tout', message="Saved screenshot") ]
        hotspot (1180, 203, 86, 25) action ShowMenu('preferences')

        # x = to hide things.
        hotspot (1242, 27, 23, 23) action [ With(dissolve), ui.callsinnewcontext("_hide_windows"), With(dissolve) ]

        # If we have who text, show it.
        if who:
            text who:
                id "who"
                pos (175, 32)
                size 30
                bold False
                outlines [ (3, "#000", 0, 0) ]
                
        # Show the what text.
        text what:
            id "what"
            area (190, 85, 952, 114)
            size 25
            outlines [ (2, "#000", 0, 0) ]    
    

