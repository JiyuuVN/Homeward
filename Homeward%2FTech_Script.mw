    def MusicSetTopTrack(value):
        """
        If value is in-range for the top track, return an action that sets the 
        top track to the value. Otherwise, return None, to make the button 
        insensitive.
        """
        
        if value < 0:
            return None
        
        if value > len(MUSIC_TRACKS) - MUSIC_TRACKS_PER_PAGE:
            return None
        
        return SetScreenVariable("top_track", value)
    
