# You can place the script of your game in this file.

screen main_menu:
    tag menu
    imagemap:
        ground "main_menu"
        idle "UI Graphics/menu_text_default.png"
        insensitive "UI Graphics/menu_text_default.png"
        hover "UI Graphics/menu_text_highlighted.png"

        hotspot (500, 10, 750, 310) action None
        hotspot (1035, 288, 1163-1035, 326-288) action Start()
        hotspot (905, 340, 1163-905, 376-340) action FileLoad("1", page="quick", confirm=False)
        hotspot (900, 395, 1163-900, 435-395) action ShowMenu('load')
        hotspot (1000, 448, 1163-1000, 483-448) action ShowMenu('preferences')
        hotspot (1030, 505, 1163-1030, 540-505) action ShowMenu("bonus_menu")
        hotspot (1060, 555, 1163-1060, 590-555) action Quit()
        
    add "UI Graphics/logo.png" pos (551, 65)

screen bonus_menu:
    tag menu
    imagemap:
        ground "UI Graphics/bonus_ground.png"
        idle "UI Graphics/bonus_idle.png"
        insensitive "UI Graphics/bonus_gound.png"
        hover "UI Graphics/bonus_hover.png"
        
        if persistent.ending_1 or persistent.ending_2 or persistent.ending_3:
            hotspot (10, 244, 1000, 50) action ShowMenu('gallery_general1')
        if persistent.ending_1 or persistent.ending_2 or persistent.ending_3:
            hotspot (10, 318, 1000, 50) action ShowMenu('music_room')
        if persistent.ending_1 and persistent.ending_2 and persistent.ending_3:
            hotspot (10, 392, 1000, 50) action ShowMenu('deleted_scenes')
        if persistent.ending_1 or persistent.ending_2 or persistent.ending_3:
            hotspot (10, 472, 1000, 50) action Start('directornotes')
        hotspot (930, 680, 1221-930, 742-680) action ShowMenu('main_menu')

label bonus_menu:
    call screen bonus_menu
        
